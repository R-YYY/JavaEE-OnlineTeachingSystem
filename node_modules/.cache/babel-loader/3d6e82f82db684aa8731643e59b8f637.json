iv>\n            <div class=\"editArea\">\n              <h2>课程信息</h2>\n              <el-form>\n                <el-form-item label=\"课程编号\">\n                  <el-input\n                    class=\"inputInfo\"\n                    v-model=\"this.$route.params.course_id\"\n                    :disabled=\"true\"\n                  >\n                  </el-input>\n                </el-form-item>\n                <el-form-item label=\"责任教师\">\n                  <el-input\n                    v-model=\"this.$route.params.course_teacher_ID\"\n                    :disabled=\"true\"\n                    class=\"inputInfo\"\n                  >\n                  </el-input>\n                </el-form-item>\n                <el-form-item label=\"课程名称\">\n                  <el-input\n                    v-model=\"tmpName\"\n                    style=\"width: 400px\"\n                  >\n                  </el-input>\n                </el-form-item>\n                <el-form-item label=\"课程介绍\">\n                  <el-input\n                    v-model=\"tmpDes\"\n                    :rows=\"4\"\n                    type=\"textarea\"\n                    resize=\"none\"\n                    style=\"width: 400px\"\n                  >\n                  </el-input>\n                </el-form-item>\n              </el-form>\n            </div>\n          </el-container>\n        </el-tab-pane>\n        <el-tab-pane label=\"教师团队\"> </el-tab-pane>\n        <el-tab-pane label=\"助教团队\"> </el-tab-pane>\n      </el-tabs>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"EditCourseInfo\",\n  data() {\n    return {\n      activeIndex: \"0\",\n      tmpName: \"\",\n      tmpDes: \"\",\n    };\n  },\n  methods: {\n    handleClick(tab, event) {\n      if (tab.index == 0)\n        this.$router.push({\n          name: \"info\",\n          params: {\n            course_id: 42024401,\n          },\n        });\n      else if (tab.index == 1)\n        this.$router.push({\n          name: \"teachers\",\n          params: {\n            course_id: 42024401,\n          },\n        });\n      else if (tab.index == 2)\n        this.$router.push({\n          name: \"tas\",\n          params: {\n            course_id: 42024401,\n          },\n        });\n    },\n    //更新课程资料\n    saveEdit() {\n      console.log(\"success!\");\n      this.$router.push({ name: \"info\" });\n    },\n    cancelEdit() {\n      this.$router.push({ name: \"info\" });\n    },\n  },\n  mounted() {\n    let _this =this\n    _this.tmpName=this.$route.params.course_name\n    _this.tmpDes=this.$route.params.course_des\n  },\n};\n</script>\n\n<style scoped>\n.editTab {\n  height: 100%;\n  margin-top: 20px;\n  margin-right: 50px;\n  margin-left: 40px;\n  background-color: white;\n}\n\n.uploadArea {\n  margin-left: 170px;\n  margin-top: 70px;\n  margin-right: 100px;\n  font-size: 12px;\n  text-align: center;\n}\n\n.editArea {\n  margin-top: 10px;\n  margin-left: 100px;\n  line-height: 90px;\n  width: 600px;\n}\n\n.btn {\n  margin-left: 40px;\n  margin-right: 20px;\n}\n\n.inputInfo {\n  width: 150px;\n  font-size: 17px;\n}\n</style>\n"],"sourceRoot":"src/components/teacherPages/course"}]}