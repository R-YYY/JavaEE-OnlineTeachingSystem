A,MAAA,CAAA;AADA;AAHA,KAAA,EAOA,IAPA,CAOA,UAAA,QAAA,EAAA;AACA,MAAA,MAAA,CAAA,UAAA,CAAA,IAAA,CACA;AACA,QAAA,KAAA,EAAA,MADA;AAEA,QAAA,QAAA,EAAA,MAAA,CAAA,aAAA,CAAA,QAAA,CAAA,IAAA,CAAA,CAAA,CAAA;AAFA,OADA,EAKA;AACA,QAAA,KAAA,EAAA,MADA;AAEA,QAAA,QAAA,EAAA,MAAA,CAAA,aAAA,CAAA,QAAA,CAAA,IAAA,CAAA,CAAA,CAAA;AAFA,OALA;AAUA,KAlBA,EAmBA,KAnBA,CAmBA,UAAA,KAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,KArBA,EAHA,CA0BA;;AACA,SAAA,MAAA,CAAA;AACA,MAAA,GAAA,EAAA,mBADA;AAEA,MAAA,MAAA,EAAA,KAFA;AAGA,MAAA,MAAA,EAAA;AACA,QAAA,SAAA,EAAA,KAAA,MAAA,CAAA,MAAA,CAAA,SADA;AAEA,QAAA,IAAA,EAAA,KAAA;AAFA;AAHA,KAAA,EAOA,IAPA,CAOA,UAAA,QAAA,EAAA;AACA,MAAA,MAAA,CAAA,QAAA,GAAA,QAAA,CAAA,IAAA;AACA,KATA;AAUA,GApMA;AAqMA,EAAA,KAAA,EAAA;AACA,IAAA,QAAA,EAAA,kBAAA,MAAA,EAAA;AAAA;;AACA,WAAA,QAAA,GAAA,EAAA;AACA,WAAA,MAAA,CAAA;AACA,QAAA,GAAA,EAAA,mBADA;AAEA,QAAA,MAAA,EAAA,KAFA;AAGA,QAAA,MAAA,EAAA;AACA,UAAA,SAAA,EAAA,KAAA,MAAA,CAAA,MAAA,CAAA,SADA;AAEA,UAAA,IAAA,EAAA;AAFA;AAHA,OAAA,EAQA,IARA,CAQA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,QAAA,CAAA,IAAA;AACA,OAVA,EAWA,KAXA;AAYA;AAfA;AArMA,CAAA","sourcesContent":["<template>\n  <div>\n    <div>\n      <el-container>\n        <el-input\n          class=\"inputFileName\"\n          v-model=\"input\"\n          placeholder=\"请输入查找文件名称\"\n        >\n        </el-input>\n        <el-button>搜索</el-button>\n        <el-tooltip\n          class=\"item\"\n          effect=\"dark\"\n          content=\"仅可在二级目录内上传文件\"\n          placement=\"right\"\n        >\n          <el-upload\n            class=\"uploadFile\"\n            action=\"#\"\n            multiple\n            :show-file-list=\"false\"\n            :disabled=\"!showOperation()\"\n            :http-request=\"handleUpload\"\n          >\n            <el-button :disabled=\"!showOperation()\">上传文件</el-button>\n          </el-upload>\n        </el-tooltip>\n      </el-container>\n    </div>\n    <div>\n      <el-tabs class=\"fileTab\" type=\"border-card\">\n        <el-tab-pane label=\"课程资料\">\n          <el-container style=\"height: 480px\">\n            <div>\n              <div class=\"pathArea\">\n                <el-breadcrumb class=\"filePath\" separator=\"/\">\n                  <el-breadcrumb-item>\n                    <span>软件工程</span>\n                  </el-breadcrumb-item>\n                  <el-breadcrumb-item v-for=\"item in pathArray()\">\n                    <span>{{ item }}</span>\n                  </el-breadcrumb-item>\n                </el-breadcrumb>\n              </div>\n              <div>\n                <!--文件树区域-->\n                <el-card class=\"fileTree\" shadow=\"never\">\n                  <el-tree\n                    :data=\"totalFiles\"\n                    default-expand-all\n                    :expand-on-click-node=\"false\"\n                    @node-click=\"handleNodeClick\"\n                  >\n                    <!--树节点-->\n                    <span class=\"treeNode\" slot-scope=\"{ node, data }\">\n                      <span class=\"fileName\"\n                        ><i class=\"el-icon-folder-opened\"></i\n                        >{{ node.label }}</span\n                      >\n                    </span>\n                  </el-tree>\n                </el-card>\n              </div>\n            </div>\n            <div class=\"fileList\">\n              <el-table\n                :data=\"fileList\"\n                height=\"450px\"\n                :row-style=\"{ height: '50px' }\"\n                :cell-style=\"{ padding: '0' }\"\n              >\n                <el-table-column prop=\"file_name\" label=\"文件名\" width=\"300px\">\n                </el-table-column>\n                <el-table-column\n                  prop=\"submit_time\"\n                  label=\"上传时间\"\n                  width=\"200px\"\n                >\n                </el-table-column>\n                <el-table-column width=\"200px\" label=\"操作\" align=\"center\">\n                  <template slot-scope=\"scope\">\n                    <el-button\n                      icon=\"el-icon-view\"\n                      size=\"small\"\n                      circle\n                      v-if=\"!showOperation()\"\n                      @click=\"showFiles(scope.row)\"\n                    >\n                    </el-button>\n                    <el-button\n                      icon=\"el-icon-download\"\n                      size=\"small\"\n                      circle\n                      v-if=\"showOperation()\"\n                      style=\"margin-right: 20px\"\n                      @click=\"handleDownload(scope.row)\"\n                    >\n                    </el-button>\n                    <el-button\n                      icon=\"el-icon-delete\"\n                      size=\"small\"\n                      circle\n                      v-if=\"showOperation()\"\n                      type=\"danger\"\n                      plain\n                      @click=\"handleDelete(scope.row)\"\n                    >\n                    </el-button>\n                  </template>\n                </el-table-column>\n              </el-table>\n            </div>\n          </el-container>\n        </el-tab-pane>\n      </el-tabs>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"ManageFile\",\n  data() {\n    return {\n      input: \"\",\n      filePath: \"/课程资料\",\n      fileList: [],\n      totalFiles: [],\n      defaultProps: {\n        children: \"children\",\n        label: \"label\",\n      },\n    };\n  },\n  methods: {\n    pathArray() {\n      return this.filePath.split(\"/\").slice(1);\n    },\n\n    handleNodeClick(data, node) {\n      let route = [];\n      while (node.parent !== null) {\n        route.push(node.label);\n        node = node.parent;\n      }\n      this.filePath = \"\";\n      for (let i = route.length - 1; i >= 0; i--) {\n        this.filePath = this.filePath + \"/\" + route[i];\n      }\n    },\n\n    showFiles(row) {\n      this.filePath = this.filePath + \"/\" + row.file_name;\n    },\n\n    showOperation() {\n      return this.filePath.split(\"/\").length >= 3;\n    },\n\n    childrenFiles(data) {\n      let result = [];\n      for (let i = 0; i < data.length; i++) {\n        result.push({\n          label: data[i],\n        });\n      }\n      return result;\n    },\n\n    handleUpload(file) {\n      let data = new FormData();\n      data.append(\"course_ID\", this.$route.params.course_id);\n      data.append(\"path\", this.filePath);\n      data.append(\"file\", file.file);\n      this.uploadFiles(data);\n    },\n\n    uploadFiles(data) {\n      this.$axios({\n        url: \"file/uploadFile\",\n        method: \"post\",\n        data: data,\n      })\n        .then((response) => {\n          if (response.data === 1) {\n            this.$message({\n              type: \"success\",\n              message: file.file.name + \" 上传成功！\",\n            });\n            this.fileList.push({\n              file_name: file.file.name,\n              submit_time: new Date().getTime(),\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: \"上传失败！请重试！\",\n            });\n          }\n        })\n        .catch(() => {\n          this.$message({\n            type: \"error\",\n            message: \"上传失败！请重试！\",\n          });\n        });\n    },\n\n    handleDelete(row) {\n      let data = new FormData();\n      data.append(\"course_ID\", this.$route.params.course_id);\n      data.append(\"path\", this.filePath);\n      data.append(\"file_name\", row.file_name);\n      this.deleteFile(data);\n    },\n\n    deleteFile(data) {\n      this.$axios({\n        url: \"/file/deleteFile\",\n        method: \"post\",\n        data: data,\n      })\n        .then((response) => {\n          if (response.data === 1) {\n            this.fileList.splice(data, 1);\n            this.$message({\n              type: \"success\",\n              message: \"删除成功!\",\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: \"删除失败!请重试！\",\n            });\n          }\n        })\n        .catch(() => {\n          this.$message({\n            type: \"error\",\n            message: \"删除失败!请重试！\",\n          });\n        });\n    },\n\n    handleDownload(row) {\n      let data = new FormData();\n      data.append(\"course_ID\", this.$route.params.course_id);\n      data.append(\"path\", this.filePath);\n      data.append(\"file_name\", row.file_name);\n      this.downloadFile(data, row.file_name);\n    },\n\n    downloadFile(data) {\n      this.$axios({\n        url: \"/file/downloadFile\",\n        method: \"post\",\n        data: data,\n        responseType: \"blob\",\n      }).then((response) => {\n        console.log(response)\n        let blob = new Blob([response.data]);\n        const disposition = response.headers['content-disposition'];\n        //获得文件名\n        let fileName = disposition.substring(disposition.indexOf('filename=') + 9, disposition.length);\n        //解码\n        fileName = decodeURI(escape(fileName));\n        if (window.navigator.msSaveOrOpenBlob) {\n          navigator.msSaveBlob(blob, fileName);\n        } else {\n          const link = document.createElement(\"a\");\n          link.href = window.URL.createObjectURL(blob);\n          link.download = fileName;\n          link.click();\n          //释放内存\n          window.URL.revokeObjectURL(link.href);\n        }\n      });\n    },\n  },\n  mounted() {\n    //加载所有二级文件\n    //暂定资料库分为三级，course（projects）-> ×××（实验项目名）文件夹 -> ×××.×××\n    this.$axios({\n      url: \"/file/getTotalFiles\",\n      method: \"get\",\n      params: {\n        course_ID: this.$route.params.course_id,\n      },\n    })\n      .then((response) => {\n        this.totalFiles.push(\n          {\n            label: \"课程资料\",\n            children: this.childrenFiles(response.data[0]),\n          },\n          {\n            label: \"实验资料\",\n            children: this.childrenFiles(response.data[1]),\n          }\n        );\n      })\n      .catch((error) => {\n        console.log(error);\n      });\n\n    //默认选中节点\n    this.$axios({\n      url: \"/file/getFileList\",\n      method: \"get\",\n      params: {\n        course_ID: this.$route.params.course_id,\n        path: this.filePath,\n      },\n    }).then((response) => {\n      this.fileList = response.data;\n    });\n  },\n  watch: {\n    filePath: function (newVal) {\n      this.fileList = [];\n      this.$axios({\n        url: \"/file/getFileList\",\n        method: \"get\",\n        params: {\n          course_ID: this.$route.params.course_id,\n          path: newVal,\n        },\n      })\n        .then((response) => {\n          this.fileList = response.data;\n        })\n        .catch();\n    },\n  },\n};\n</script>\n\n<style scoped>\n.fileTab {\n  height: 100%;\n  margin-top: 20px;\n  margin-right: 50px;\n  margin-left: 40px;\n  background-color: white;\n}\n\n.pathArea {\n  margin-left: 30px;\n  margin-top: 10px;\n}\n\n.filePath {\n  font-size: 17px;\n}\n\n.inputFileName {\n  margin-left: 40px;\n  width: 300px;\n}\n\n.fileTree {\n  margin-top: 20px;\n  margin-left: 25px;\n  margin-right: 50px;\n  width: 400px;\n  height: 420px;\n}\n\n.fileList {\n  margin-top: 30px;\n  height: 430px;\n}\n\n.treeNode {\n  font-size: 15px;\n}\n\n.uploadFile {\n  margin-left: 20px;\n}\n\n.folderBtn {\n  margin-left: 50px;\n}\n</style>\n"],"sourceRoot":"src/components/teacherPages/file"}]}