message({\r\n          type: 'info',\r\n          message: '取消添加'\r\n        });\r\n      });\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n.studentTab{\r\n  height: 100%;\r\n  margin-top: 20px;\r\n  margin-right: 50px;\r\n  margin-left: 40px;\r\n  background-color:white;\r\n}\r\n\r\n.addStudent{\r\n  position: relative;\r\n  margin-top: 10px;\r\n  margin-left: 70px;\r\n  margin-right: 50px;\r\n}\r\n\r\n.memberTable{\r\n  width: 1060px;\r\n  margin-left: 70px;\r\n  margin-top: 10px;\r\n}\r\n\r\n</style>"],"sourceRoot":"src/components/teacherPages/student"}]}