,SAAA,EAAA,EADA;AAEA,QAAA,SAAA,EAAA;AAFA;AADA,KAAA,EAKA,IALA,CAKA,UAAA,QAAA,EAAA;AACA,MAAA,KAAA,CAAA,aAAA,GAAA,QAAA,CAAA,IAAA;AACA,KAPA;AAQA,GAvBA;AAwBA,EAAA,OAAA,EAAA;AACA,IAAA,eADA,2BACA,aADA,EACA;AACA,UAAA,aAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,eAAA,IAAA;AACA,OAFA,MAGA;AACA,eAAA,KAAA;AACA;AACA,KARA;AASA,IAAA,YATA,wBASA,GATA,EASA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,KAXA;AAYA,IAAA,cAZA,0BAYA,UAZA,EAYA,QAZA,EAYA;AACA,UAAA,EAAA,GAAA,KAAA,SAAA;AACA,UAAA,IAAA,GAAA,WAAA,UAAA;AACA,UAAA,IAAA,GAAA,IAAA,QAAA,EAAA;AACA,MAAA,IAAA,CAAA,MAAA,CAAA,WAAA,EAAA,EAAA;AACA,MAAA,IAAA,CAAA,MAAA,CAAA,MAAA,EAAA,IAAA;AACA,MAAA,IAAA,CAAA,MAAA,CAAA,WAAA,EAAA,QAAA;AACA,WAAA,YAAA,CAAA,IAAA,EAAA,QAAA;AACA,KApBA;AAqBA,IAAA,YArBA,wBAqBA,IArBA,EAqBA;AACA,WAAA,MAAA,CAAA;AACA,QAAA,GAAA,EAAA,oBADA;AAEA,QAAA,MAAA,EAAA,MAFA;AAGA,QAAA,IAAA,EAAA,IAHA;AAIA,QAAA,YAAA,EAAA;AAJA,OAAA,EAKA,IALA,CAKA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,YAAA,IAAA,GAAA,IAAA,IAAA,CAAA,CAAA,QAAA,CAAA,IAAA,CAAA,CAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,YAAA,WAAA,GAAA,QAAA,CAAA,OAAA,CAAA,qBAAA,CAAA,CAJA,CAKA;;AACA,YAAA,QAAA,GAAA,WAAA,CAAA,SAAA,CACA,WAAA,CAAA,OAAA,CAAA,WAAA,IAAA,CADA,EAEA,WAAA,CAAA,MAFA,CAAA,CANA,CAUA;;AACA,QAAA,QAAA,GAAA,SAAA,CAAA,QAAA,CAAA;;AACA,YAAA,MAAA,CAAA,SAAA,CAAA,gBAAA,EAAA;AACA,UAAA,SAAA,CAAA,UAAA,CAAA,IAAA,EAAA,QAAA;AACA,SAFA,MAEA;AACA,cAAA,IAAA,GAAA,QAAA,CAAA,aAAA,CAAA,GAAA,CAAA;AACA,UAAA,IAAA,CAAA,IAAA,GAAA,MAAA,CAAA,GAAA,CAAA,eAAA,CAAA,IAAA,CAAA;AACA,UAAA,IAAA,CAAA,QAAA,GAAA,QAAA;AACA,UAAA,IAAA,CAAA,KAAA,GAJA,CAKA;;AACA,UAAA,MAAA,CAAA,GAAA,CAAA,eAAA,CAAA,IAAA,CAAA,IAAA;AACA;AACA,OA3BA;AA4BA;AAlDA;AAxBA,CAAA","sourcesContent":["<template>\r\n  <div id=\"CourseFile\">\r\n    <div class=\"FolderCard\" v-for=\"info in project_files\" v-if=\"show_folderName(info.fileName)\">\r\n      <div class=\"projectName\" style=\"font-size: 18px;\">【{{info.folderName}}】</div>\r\n      <div v-for=\"file in info.fileName\" class=\"fileName\" @click=\"handleDownload(info.folderName,file)\">\r\n        {{file}}\r\n      </div>\r\n      <el-divider></el-divider>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"ProjectFile\",\r\n  props:{\r\n    course_id:{\r\n      type: String,\r\n      default: '',\r\n    },\r\n  },\r\n  data(){\r\n    return{\r\n      project_files: [],\r\n    }\r\n  },\r\n  mounted() {\r\n    var id=this.course_id;\r\n    this.$axios.get('/file/getDirectoryFiles',{\r\n      params:{\r\n        course_ID:id,\r\n        isProject:1,\r\n      }\r\n    }).then((response)=>{\r\n      this.project_files=response.data;\r\n    })\r\n  },\r\n  methods: {\r\n    show_folderName(fileName_list){\r\n      if(fileName_list.length>0){\r\n        return true;\r\n      }\r\n      else{\r\n        return false;\r\n      }\r\n    },\r\n    handleChange(val) {\r\n      console.log(val);\r\n    },\r\n    handleDownload(folderName,fileName) {\r\n      var id=this.course_id;\r\n      var path='/实验资料/'+folderName;\r\n      let data = new FormData();\r\n      data.append(\"course_ID\", id);\r\n      data.append(\"path\", path);\r\n      data.append(\"file_name\", fileName);\r\n      this.downloadFile(data, fileName);\r\n    },\r\n    downloadFile(data) {\r\n      this.$axios({\r\n        url: \"/file/downloadFile\",\r\n        method: \"post\",\r\n        data: data,\r\n        responseType: \"blob\",\r\n      }).then((response) => {\r\n        console.log(response);\r\n        let blob = new Blob([response.data]);\r\n        console.log(blob);\r\n        const disposition = response.headers[\"content-disposition\"];\r\n        //获得文件名\r\n        let fileName = disposition.substring(\r\n            disposition.indexOf(\"filename=\") + 9,\r\n            disposition.length\r\n        );\r\n        //解码\r\n        fileName = decodeURI(fileName);\r\n        if (window.navigator.msSaveOrOpenBlob) {\r\n          navigator.msSaveBlob(blob, fileName);\r\n        } else {\r\n          const link = document.createElement(\"a\");\r\n          link.href = window.URL.createObjectURL(blob);\r\n          link.download = fileName;\r\n          link.click();\r\n          //释放内存\r\n          window.URL.revokeObjectURL(link.href);\r\n        }\r\n      });\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.FolderCard{\r\n  margin-top: 30px;\r\n  margin-left: 50px;\r\n  margin-right: 50px;\r\n}\r\n.fileName{\r\n  margin-top: 15px;\r\n  margin-left: 30px;\r\n  color: rgba(11,159,250,1);\r\n  cursor: context-menu;\r\n}\r\n.fileName:hover{\r\n  color: rgba(0,0,250,0.6);\r\n}\r\na:hover{\r\n  text-decoration: none;\r\n}\r\n</style>"],"sourceRoot":"src/components/studentPages/file"}]}