";\n    } else if (\n      this.$route.name === \"totalGrades\" ||\n      this.$route.name === \"partGrades\" ||\n      this.$route.name === \"setGrades\"\n    ) {\n      this.activeIndex = \"4\";\n    } else if (this.$route.name === \"files\") {\n      this.activeIndex = \"5\";\n    } else if (this.$route.name === \"feedbacks\") {\n      this.activeIndex = \"6\";\n    }\n  },\n};\n</script>\n\n<style scoped>\n.option {\n  width: 170px;\n  height: 70px;\n  font-size: 17px;\n}\n\n#courseImg {\n  margin-top: 5px;\n  width: 170px;\n}\n\n.mainAside {\n  text-align: center;\n  background-color: #545c64;\n}\n\n.title {\n  font-size: 35px;\n  line-height: 10px;\n  text-align: left;\n}\n</style>\n"],"sourceRoot":"src/components/teacherPages"}]}