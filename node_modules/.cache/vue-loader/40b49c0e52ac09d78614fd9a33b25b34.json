leteBtn\" type=\"danger\" plain>\n        <span>删除课程</span>\n      </el-button>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"CourseInfo\",\n  data() {\n    return {};\n  },\n  methods: {\n    editCourse(e) {\n      this.$router.push({\n        name: \"edit\",\n        params: {\n          course_id: 42024401,\n          course_name: document\n            .getElementById(\"courseName\")\n            .innerText.split(\"：\")[1],\n          course_teacher_ID: document\n            .getElementById(\"courseTeacher\")\n            .innerText.split(\"：\")[1],\n          course_des: document\n            .getElementById(\"courseDes\")\n            .innerText.split(\"：\")[1],\n        },\n      });\n    },\n  },\n  mounted: function () {\n    let _this = this;\n    this.$axios\n      .get(\"/course/get\", {\n        params: {\n          course_ID: 42024401,\n        },\n      })\n      .then(function (response) {\n        document.getElementById(\"courseTeacher\").innerHTML +=\n          response.data.teacher_ID;\n        document.getElementById(\"courseName\").innerHTML += response.data.name;\n        document.getElementById(\"courseDes\").innerHTML +=\n          response.data.description;\n      })\n      .catch(function (error) {\n        _this.courseInfo.name = \"\";\n        _this.courseInfo.description = \"\";\n      });\n  },\n};\n</script>\n\n<style scoped>\n#courseID,\n#courseName,\n#courseTeacher,\n#courseDes {\n  margin-top: 35px;\n  margin-bottom: 30px;\n}\n\n.editBtn,\n.endBtn,\n.deleteBtn {\n  margin-top: 60px;\n  margin-left: 30px;\n  margin-right: 40px;\n}\n</style>\n"]}]}